   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "pageRank = nx.pagerank(graph, alpha = 0.9)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
