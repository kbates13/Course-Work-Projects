    "with open(\"stack_overflow_edgelist.csv\") as file:\n",
    "    for line in file:\n",
    "        if 'source' in line:\n",
    "            continue\n",
    "            \n",
    "        else:\n",
    "            line = line.strip().split(',')\n",
    "            graph.add_edge((line[0]), (line[1]), weight = float(line[2]))\n",
    "            \n",
    "        \n",
    "        "
   ]
